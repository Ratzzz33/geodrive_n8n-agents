      "id": "get-recent-bookings",
      "name": "Get Recent Bookings",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [740, 500],
      "continueOnFail": true
    },
    {
      "parameters": {
        "jsCode": "const branch = $('Prepare Branches').item.json.branch;\n\n// Безопасное получение данных со всех входов\nconst inputs = $input.all();\nlet companyCash = null;\nlet bookings = null;\n\n// Проходим по всем входным данным\ninputs.forEach(input => {\n  const json = input.json;\n  \n  // Определяем, откуда пришли данные\n  if (json && json.data && Array.isArray(json.data)) {\n    // Это данные о bookings\n    bookings = json;\n  } else if (json && (json.error || json.counts || json.cash_gel !== undefined)) {\n    // Это данные о кассе компании\n    companyCash = json;\n  }\n});\n\n// Обработка кассы компании\nlet cashData = [];\nif (companyCash && !companyCash.error) {\n  // company_counts_v2 возвращает {counts: {data: [...]}}\n  let payments = [];\n  if (companyCash.counts && companyCash.counts.data && Array.isArray(companyCash.counts.data)) {\n    payments = companyCash.counts.data;\n  } else if (companyCash.counts && Array.isArray(companyCash.counts)) {\n    payments = companyCash.counts;\n  }\n  \n  payments.forEach(item => {\n    // Извлекаем attributes (новый формат) или используем item напрямую (старый формат)\n    const payment = item.attributes || item;\n    cashData.push({\n      branch,\n      type: 'company_cash',\n      payment_id: payment.id,\n      sum: payment.sum,\n      cash: payment.cash,\n      cashless: payment.cashless,\n      group: payment.group,\n      subgroup: payment.subgroup,\n      description: payment.description,\n      car_id: payment.car_id,\n      booking_id: payment.booking_id,\n      client_id: payment.client_id,\n      user_id: payment.user_id,\n      created_at: payment.created_at,\n      raw_data: JSON.stringify(payment)\n    });\n  });\n}\n\n// Обработка событий (bookings)\nlet eventsData = [];\nif (bookings && Array.isArray(bookings.data)) {\n  bookings.data.forEach(booking => {\n    eventsData.push({\n      branch,\n      type: 'booking_event',\n      booking_id: booking.id,\n      event_type: booking.state,\n      car_id: booking.car_id,\n      client_id: booking.client_id,\n      issue_at: booking.issue_at,\n      return_at: booking.return_at,\n      raw_data: JSON.stringify(booking)\n    });\n  });\n}\n\nconst result = [];\ncashData.forEach(c => result.push({ json: c }));\neventsData.forEach(e => result.push({ json: e }));\n\nreturn result.length > 0 ? result : [{ json: { branch, status: 'no_data' } }];"
      },
