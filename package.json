{
  "name": "jarvis-bot",
  "version": "0.1.0",
  "description": "Telegram-бот Jarvis для управления автопрокатом Geodrive",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:api": "tsx watch src/api-only.ts",
    "dev:playwright": "tsx watch src/services/playwrightService.ts",
    "dev:scraper": "tsx watch src/services/httpScraperService.ts",
    "start:api": "tsx src/api-only.ts",
    "start:playwright": "node dist/services/playwrightService.js",
    "start:scraper": "node dist/services/httpScraperService.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "reset-webhook": "tsx setup/reset-bot-webhook.ts",
    "lint": "echo 'Linting not configured yet'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "telegram",
    "bot",
    "rental",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@types/express": "^5.0.5",
    "axios": "^1.13.1",
    "axios-cookiejar-support": "^5.0.3",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.4",
    "express": "^5.1.0",
    "jsdom": "^27.1.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "playwright": "^1.49.0",
    "postgres": "^3.4.4",
    "telegraf": "^4.16.3",
    "tough-cookie": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/node-cron": "^3.0.11",
    "@vitest/coverage-v8": "^2.1.8",
    "drizzle-kit": "^0.31.6",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  }
}
